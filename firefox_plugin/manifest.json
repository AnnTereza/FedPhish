{

    "description": "Native messaging example add-on",
    "manifest_version": 2,
    "name": "fedphish",
    "version": "1.0",
    "icons": {
      "48": "icons/icon_48.png"
    },
  
    "browser_specific_settings": {
      "gecko": {
        "id": "fedphish@example.org",
        "strict_min_version": "50.0"
      }
    },
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_action": {
      "default_icon": "icons/icon_48.png"
    },
    
    "permissions": [
      "nativeMessaging",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ]  
  }